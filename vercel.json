{
  "version": 2,
  "builds": [
    {
      "src": "api/test_minimal.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "pythonRequirements": "api/requirements-minimal.txt"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "api/test_minimal.py"
    }
  ]
}
